node_modules/
.pnp
.pnp.js

# Testing
coverage/
*.test.ts
*.test.tsx
*.spec.ts
*.spec.tsx
__tests__/

# Production
.next/
out/
dist/
build/

# Misc
.DS_Store
*.pem
*.log
*.local

# Debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Local env files (use Fly.io secrets instead)
.env
.env*.local

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# Git
.git/
.gitignore

# Documentation
docs/
*.md
!README.md

