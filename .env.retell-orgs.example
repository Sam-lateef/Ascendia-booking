# Organization Slug Mapping for Retell Multi-Org Support
# Add this to your .env file or configure in websocket-handler.ts

# Format: ORG_SLUG_MAP in src/retell/websocket-handler.ts
# 
# const ORG_SLUG_MAP: Record<string, string> = {
#   'default': '',  // Uses getCachedDefaultOrganizationId()
#   'org-a': 'b445a9c7-af93-4b4a-a975-40d3f44178ec',
#   'org-b': 'another-org-id-here',
#   'clinic-a': 'uuid-for-clinic-a',
#   'clinic-b': 'uuid-for-clinic-b',
# };

# Example Usage:
# Org A Retell Agent → WebSocket URL: wss://ascendiaai-websocket.fly.dev/llm-websocket/org-a
# Org B Retell Agent → WebSocket URL: wss://ascendiaai-websocket.fly.dev/llm-websocket/org-b

# To get your organization IDs:
# SELECT id, name, slug FROM organizations;
